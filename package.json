{
  "name": "xophz-compass",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint --watch",
    "serve-localhost": "vue-cli-service serve --host localhost",
    "serve:bs": "vue-cli-service serve:bs",
    "test:unit": "vue-cli-service test:unit",
    "watch-build": "vue-cli-service build --mode production --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/pro-light-svg-icons": "^5.14.0",
    "@fortawesome/pro-regular-svg-icons": "^5.14.0",
    "@fortawesome/pro-solid-svg-icons": "^5.14.0",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "@vue/cli": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "core-js-pure": "^3.6.5",
    "cross-env": "^5.2.1",
    "epic-spinners": "^1.1.0",
    "lodash": "^4.17.19",
    "nprogress": "^0.2.0",
    "roboto-fontface": "*",
    "vue": "^2.6.11",
    "vue-friendly-iframe": "^0.13.0",
    "vue-jest": "^3.0.6",
    "vue-loader": "^15.9.3",
    "vue-lodash": "^2.1.2",
    "vue-moment": "^4.1.0",
    "vue-nprogress": "^0.1.5",
    "vue-router": "^3.3.4",
    "vuetify": "^2.3.4",
    "vuex": "^3.5.1",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.4",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^3.12.1",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "chai": "^4.1.2",
    "deepmerge": "^4.2.2",
    "eslint": "^5",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^4.0.3",
    "imports-loader": "^0.8.0",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.5",
    "regenerator-runtime": "^0.13.5",
    "sass": "^1.26.10",
    "sass-loader": "^7.3.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vue-cli-plugin-browser-sync": "^1.0.0-rc5",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.4.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.6.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "eslint:recommended",
      "@vue/airbnb",
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue",
      "jsx"
    ],
    "verbose": false,
    "collectCoverage": true,
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.js?$": "babel-jest"
    },
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
